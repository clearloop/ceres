{
  "compilerOptions": {
    "outDir": "./app/",
    "noImplicitAny": true,
    "module": "commonjs",
    "target": "es5",
    "jsx": "react",
    "allowJs": true,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true
  },
  "include": ["ui/**/*", "native/**/*", "scripts/**/*"]
}
