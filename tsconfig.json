{
  "compilerOptions": {
    "lib": ["dom"],
    "outDir": "./app/",
    "noImplicitAny": true,
    "target": "es2017",
    "jsx": "react",
    "allowJs": true,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "skipLibCheck": true
  },
  "exclude": ["app/**/*", "builder/**/*", "native/**/*.ts"],
  "extends": "@tsconfig/svelte/tsconfig.json"
}
